<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="html">
	<inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />
    <inherits name="com.github.antag99.retinazer_gwt"/>

	<inherits name='Heaven' />
	<entry-point class='com.github.antag99.heaven.client.HtmlLauncher' />
	
	<set-configuration-property name="gdx.assetpath" value="../core/assets" />
    <extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.g2d"/>
    <extend-configuration-property name="gdx.reflect.include" value="com.github.antag99.heaven"/>
    <extend-configuration-property name="gdx.reflect.include" value="com.github.antag99.heaven.system"/>
    <extend-configuration-property name="gdx.reflect.include" value="com.github.antag99.heaven.component"/>
    <extend-configuration-property name="gdx.reflect.include" value="com.github.antag99.heaven.util"/>
</module>
